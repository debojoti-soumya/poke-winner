{
  "manifest_version": 3,
  "name": "History Sender",
  "version": "1.1",
  "description": "An extension to send browser history to a local Python script every minute.",
  "permissions": [
    "history",
    "alarms",
    "bookmarks"
  ],
  "host_permissions": [
    "https://poke-winner-gimran.onrender.com/"
  ],
  "background": {
    "service_worker": "background.js"
  }
}